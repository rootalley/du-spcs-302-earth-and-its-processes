        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;p&gt;Imagine that you found two sandstone beds about 200 kilometers apart. The rock in one region was similar to the rock in the other, but one bed was several million years older than the other. From this information, you would conclude that:&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="3224">
                <material>
                  <mattext texttype="text/plain">the two beds were lithologically correlated.</mattext>
                </material>
              </response_label>
              <response_label ident="9426">
                <material>
                  <mattext texttype="text/plain">the two beds were time correlated.</mattext>
                </material>
              </response_label>
              <response_label ident="7462">
                <material>
                  <mattext texttype="text/plain">both beds would contain the same index fossils.</mattext>
                </material>
              </response_label>
              <response_label ident="1490">
                <material>
                  <mattext texttype="text/plain">Both: the two beds were time correlated and both beds would contain the same index fossils.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">3224</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
